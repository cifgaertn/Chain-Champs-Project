{
  "canisters": {
    "factorybot_backend": {
      "type": "motoko",
      "main": "src/factorybot_backend/main.mo"
    },
    "factorybot_frontend": {
      "type": "assets",
      "source": [
        "dist/factorybot_frontend/"
      ],
      "declarations": {
        "output": "src/declarations/"
      },
      "dependencies": [
        "factorybot_backend"
      ]
    }
  },
  "defaults": {
    "build": {
      "args": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    },
    "ic": {
      "providers": [
        "https://ic0.app"
      ],
      "type": "persistent"
    }
  },
  "version": 1
}
